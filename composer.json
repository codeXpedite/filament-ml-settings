{
    "name": "codexpedite/filament-ml-settings",
    "description": "Multilingual settings management package for Filament Admin Panel",
    "keywords": ["laravel", "filament", "settings", "multilingual", "translations"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CodeXpedite",
            "email": "info@codexpedite.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "astrotomic/laravel-translatable": "^11.0",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "CodeXpedite\\FilamentMlSettings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeXpedite\\FilamentMlSettings\\FilamentMlSettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "CodeXpedite\\FilamentMlSettings\\Facades\\Settings"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}